{
  "configurations": [
    {
      "name": "Launch via pnpm vue3 webpack",
      "request": "launch",
      "runtimeArgs": ["run-script", "dev:vue3Webpack"],
      "runtimeExecutable": "pnpm",
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "name": "Launch via pnpm react",
      "request": "launch",
      "runtimeArgs": ["run-script", "dev:react"],
      "stopOnEntry": true, //é¦–è¡Œæ–­
      "console": "integratedTerminal",
      "runtimeExecutable": "pnpm",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },

    {
      "name": "Launch Node Program",
      "program": "${workspaceFolder}/packages/node-debug-demo/index.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "stopOnEntry": true, //é¦–è¡Œæ–­
      "type": "node"
    },
    {
      "name": "Attach Node",
      "port": 8888,
      "request": "attach",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },

    {
      "name": "Launch React webpack",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:5555",
      "webRoot": "${workspaceFolder}",
      "file": "${workspaceFolder}/packages/webpack-react-debug/index.html",
      "sourceMapPathOverrides": {
        "meteor://ðŸ’»app/*": "${workspaceFolder}/packages/*",
        "webpack:///./~/*": "${workspaceFolder}/node_modules/*",
        "webpack://*": "${workspaceFolder}/packages/*"
      }
    },
    {
      "name": "Launch vue3",
      "request": "launch",
      "type": "chrome",
      // "runtimeArgs": ["--auto-open-devtools-for-tabs "],
      "userDataDir": false,
      "runtimeExecutable": "canary",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Launch vue webpack",
      "request": "launch",
      "type": "chrome",
      "runtimeExecutable": "canary",
      "userDataDir": false,
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "meteor://ðŸ’»app/*": "${workspaceFolder}/packages/*",
        "webpack:///./~/*": "${workspaceFolder}/node_modules/*",
        "webpack://*": "${workspaceFolder}/packages/*"
      }
    },
    {
      "name": "Attach to Chrome",
      "port": 9222,
      "request": "attach",
      "type": "chrome",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Launch react",
      "request": "launch",
      "runtimeExecutable": "canary",
      // "runtimeArgs": ["--auto-open-devtools-for-tabs "],
      "userDataDir": false,
      "type": "chrome",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
