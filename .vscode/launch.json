{
  "configurations": [
    {
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}",
      "request": "attach",
      "skipFiles": ["<node_internals>/**"],

      "type": "node",
      "presentation": {
        "hidden": false,
        "group": "attach Node",
        "order": 1
      }
    },
    {
      "name": "Launch via NPM nest/sample",
      "request": "launch",
      "runtimeArgs": ["run-script", "start"],
      "runtimeExecutable": "npm",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/packages/nest/sample/01-cats-app/",
      "skipFiles": ["<node_internals>/**"],
      "resolveSourceMapLocations": ["${workspaceFolder}/**"],
      "type": "node",
      "presentation": {
        "hidden": false,
        "group": "launch Node",
        "order": 1
      }
    },
    {
      "name": "Launch via pnpm nest",
      "request": "launch",
      "runtimeArgs": ["run-script", "dev:nest"],
      "runtimeExecutable": "pnpm",
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"],
      "presentation": {
        "hidden": false,
        "group": "launch Node",
        "order": 1
      },
      "type": "node"
    },
    {
      "name": "Launch via pnpm vue3 webpack",
      "request": "launch",
      "runtimeArgs": ["run-script", "dev:vue3Webpack"],
      "runtimeExecutable": "pnpm",
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"],
      "presentation": {
        "hidden": false,
        "group": "launch Node",
        "order": 1
      },
      "type": "node"
    },
    {
      "name": "Launch via npm vue3 debug vite",
      "request": "launch",
      "runtimeArgs": ["run-script", "dev"],
      "cwd": "${workspaceFolder}/packages/vue-debug-demo2/",
      "runtimeExecutable": "npm",
      "console": "integratedTerminal",
      "resolveSourceMapLocations": ["${workspaceFolder}/**"],
      "skipFiles": ["<node_internals>/**"],
      "presentation": {
        "hidden": false,
        "group": "launch Node",
        "order": 1
      },
      "type": "node"
    },
    {
      "name": "Launch via pnpm react",
      "request": "launch",
      "runtimeArgs": ["run-script", "dev:react"],
      "stopOnEntry": true, //首行断
      "console": "integratedTerminal",
      "runtimeExecutable": "pnpm",
      "skipFiles": ["<node_internals>/**"],
      "presentation": {
        "hidden": false,
        "group": "launch Node",
        "order": 1
      },
      "type": "node"
    },
    {
      "name": "eslint 调试",
      "program": "${workspaceFolder}/packages/shell-debug-demo/node_modules/.bin/eslint",
      "args": ["./index.js", "--fix"],
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/packages/shell-debug-demo",
      "request": "launch",
      "type": "node",
      "presentation": {
        "hidden": false,
        "group": "launch Node",
        "order": 1
      }
    },
    {
      "name": "调试 ts 源码 需要下载源码 然后npm run build:compiler",
      "program": "${workspaceFolder}/packages/typescript/built/local/tsc.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "cwd": "${workspaceFolder}/packages/typescript",
      "console": "integratedTerminal",
      "args": ["../ts-debug-demo/input.ts"],
      "type": "node",
      "presentation": {
        "hidden": false,
        "group": "launch Node",
        "order": 1
      }
    },
    {
      "name": "调试 ts 源码 api 形式 需要npm run build:compiler",
      "program": "${workspaceFolder}/packages/ts-debug-demo/test.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/packages/ts-debug-demo",

      "args": ["./input.ts"],
      "type": "node",
      "presentation": {
        "hidden": false,
        "group": "launch Node",
        "order": 1
      }
    },
    {
      "name": "断点类型尝试",
      "program": "${workspaceFolder}/packages/break-point-debug-demo/test.js",
      "request": "launch",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/packages/break-point-debug-demo",
      "type": "node",
      "presentation": {
        "hidden": false,
        "group": "launch Node",
        "order": 1
      }
    },
    {
      "name": "断点类型尝试-网页版",
      "file": "${workspaceFolder}/packages/break-point-debug-demo/index.html",
      "request": "launch",
      "cwd": "${workspaceFolder}/packages/break-point-debug-demo",
      "type": "chrome",
      "presentation": {
        "hidden": false,
        "group": "launch Node",
        "order": 1
      }
    },
    {
      "name": "调试 patch-package 生成差异 需要下载patch-package源码 进行build",
      "program": "${workspaceFolder}/packages/patch-package/dist/index.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "cwd": "${workspaceFolder}/packages/patch-package",
      "console": "integratedTerminal",
      "args": ["acorn"],
      "type": "node",
      "presentation": {
        "hidden": false,
        "group": "launch Node",
        "order": 1
      }
    },
    {
      "name": "调试 patch-package 应用差异 需要下载patch-package源码 进行build",
      "program": "${workspaceFolder}/packages/patch-package/dist/index.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "cwd": "${workspaceFolder}/packages/patch-package",
      "console": "integratedTerminal",
      "type": "node",
      "presentation": {
        "hidden": false,
        "group": "launch Node",
        "order": 1
      }
    },
    {
      "name": "调试 eslint api",
      "program": "${workspaceFolder}/packages/shell-debug-demo/eslintApiDebugger.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal",
      "type": "node",
      "presentation": {
        "hidden": false,
        "group": "launch Node",
        "order": 1
      }
    },
    {
      "name": "调试 babel api",
      "program": "${workspaceFolder}/packages/babel-debug-demo/index.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/packages/babel-debug-demo",

      "type": "node",
      "resolveSourceMapLocations": ["${workspaceFolder}/**"],
      "presentation": {
        "hidden": false,
        "group": "launch Node",
        "order": 1
      }
    },
    {
      "name": "Launch Node Program",
      "program": "${workspaceFolder}/packages/node-debug-demo/index.js",
      "request": "launch",
      "args": ["--aaa", "--aaa2"],
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"],
      "stopOnEntry": true, //首行断
      "presentation": {
        "hidden": false,
        "group": "launch Node",
        "order": 1
      },
      "type": "node"
    },
    {
      "name": "Attach Node",
      "port": 8888,
      "request": "attach",
      "restart": {
        "delay": 1000,
        "maxAttempts": 3
      },
      "autoAttachChildProcesses": true,
      "skipFiles": ["<node_internals>/**"],
      "presentation": {
        "hidden": false,
        "group": "attach Node",
        "order": 1
      },
      "type": "node"
    },

    {
      "name": "Launch performationTest",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:5555",
      "webRoot": "${workspaceFolder}",
      "runtimeArgs": ["--auto-open-devtools-for-tabs"],
      "file": "${workspaceFolder}/packages/webpack-react-debug/performationTest.html",
      "presentation": {
        "hidden": false,
        "group": "launch chrome",
        "order": 1
      },
      "sourceMapPathOverrides": {
        "meteor://💻app/*": "${workspaceFolder}/packages/*",
        "webpack:///./~/*": "${workspaceFolder}/node_modules/*",
        "webpack://*": "${workspaceFolder}/packages/*"
      }
    },
    {
      "name": "Launch React webpack",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:5555",
      "webRoot": "${workspaceFolder}",
      "file": "${workspaceFolder}/packages/webpack-react-debug/index.html",
      "presentation": {
        "hidden": false,
        "group": "launch chrome",
        "order": 1
      },
      "sourceMapPathOverrides": {
        "meteor://💻app/*": "${workspaceFolder}/packages/*",
        "webpack:///./~/*": "${workspaceFolder}/node_modules/*",
        "webpack://*": "${workspaceFolder}/packages/*"
      }
    },
    {
      "name": "Launch vue3",
      "request": "launch",
      "type": "chrome",
      // "runtimeArgs": ["--auto-open-devtools-for-tabs "],
      "userDataDir": false,
      "runtimeExecutable": "canary",
      "presentation": {
        "hidden": false,
        "group": "launch chrome",
        "order": 1
      },
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Launch vue webpack",
      "request": "launch",
      "type": "chrome",
      "runtimeExecutable": "canary",
      "presentation": {
        "hidden": false,
        "group": "launch chrome",
        "order": 1
      },
      "userDataDir": false,
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "meteor://💻app/*": "${workspaceFolder}/packages/*",
        "webpack:///./~/*": "${workspaceFolder}/node_modules/*",
        "webpack://*": "${workspaceFolder}/packages/*"
      }
    },
    {
      "name": "Attach to Chrome",
      "port": 9222,
      "request": "attach",
      "type": "chrome",
      "presentation": {
        "hidden": false,
        "group": "Attach chrome",
        "order": 1
      },
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Launch react",
      "request": "launch",
      "runtimeExecutable": "canary",
      "presentation": {
        "hidden": false,
        "group": "launch chrome",
        "order": 1
      },
      // "runtimeArgs": ["--auto-open-devtools-for-tabs "],
      "userDataDir": false,
      "type": "chrome",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "meteor://💻app/*": "${workspaceFolder}/packages/*",
        "webpack:///./~/*": "${workspaceFolder}/node_modules/*",
        "webpack://*": "${workspaceFolder}/packages/*"
      }
    }
  ]
}
