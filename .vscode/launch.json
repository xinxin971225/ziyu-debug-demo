{
  "configurations": [
    {
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}",
      "request": "attach",
      "skipFiles": ["<node_internals>/**"],

      "type": "node",
      "presentation": {
        "hidden": false,
        "group": "attach Node",
        "order": 1
      }
    },
    {
      "name": "Launch via NPM nest/sample",
      "request": "launch",
      "runtimeArgs": ["run-script", "start"],
      "runtimeExecutable": "npm",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/packages/nest/sample/01-cats-app/",
      "skipFiles": ["<node_internals>/**"],
      "resolveSourceMapLocations": ["${workspaceFolder}/**"],
      "type": "node",
      "presentation": {
        "hidden": false,
        "group": "launch Node",
        "order": 1
      }
    },
    {
      "name": "Launch via pnpm nest",
      "request": "launch",
      "runtimeArgs": ["run-script", "dev:nest"],
      "runtimeExecutable": "pnpm",
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"],
      "presentation": {
        "hidden": false,
        "group": "launch Node",
        "order": 1
      },
      "type": "node"
    },
    {
      "name": "Launch via pnpm vue3 webpack",
      "request": "launch",
      "runtimeArgs": ["run-script", "dev:vue3Webpack"],
      "runtimeExecutable": "pnpm",
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"],
      "presentation": {
        "hidden": false,
        "group": "launch Node",
        "order": 1
      },
      "type": "node"
    },
    {
      "name": "Launch via pnpm vue3 debug vite",
      "request": "launch",
      "runtimeArgs": ["run-script", "dev:vue3"],
      "runtimeExecutable": "pnpm",
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"],
      "presentation": {
        "hidden": false,
        "group": "launch Node",
        "order": 1
      },
      "type": "node"
    },
    {
      "name": "Launch via pnpm react",
      "request": "launch",
      "runtimeArgs": ["run-script", "dev:react"],
      "stopOnEntry": true, //È¶ñË°åÊñ≠
      "console": "integratedTerminal",
      "runtimeExecutable": "pnpm",
      "skipFiles": ["<node_internals>/**"],
      "presentation": {
        "hidden": false,
        "group": "launch Node",
        "order": 1
      },
      "type": "node"
    },
    {
      "name": "eslint Ë∞ÉËØï",
      "program": "${workspaceFolder}/packages/shell-debug-demo/node_modules/.bin/eslint",
      "args": ["./index.js", "--fix"],
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/packages/shell-debug-demo",
      "request": "launch",
      "type": "node",
      "presentation": {
        "hidden": false,
        "group": "launch Node",
        "order": 1
      }
    },
    {
      "name": "Ë∞ÉËØï patch-package ÁîüÊàêÂ∑ÆÂºÇ ÈúÄË¶Å‰∏ãËΩΩpatch-packageÊ∫êÁ†Å ËøõË°åbuild",
      "program": "${workspaceFolder}/packages/patch-package/dist/index.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "cwd": "${workspaceFolder}/packages/patch-package",
      "console": "integratedTerminal",
      "args": ["acorn"],
      "type": "node",
      "presentation": {
        "hidden": false,
        "group": "launch Node",
        "order": 1
      }
    },
    {
      "name": "Ë∞ÉËØï patch-package Â∫îÁî®Â∑ÆÂºÇ ÈúÄË¶Å‰∏ãËΩΩpatch-packageÊ∫êÁ†Å ËøõË°åbuild",
      "program": "${workspaceFolder}/packages/patch-package/dist/index.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "cwd": "${workspaceFolder}/packages/patch-package",
      "console": "integratedTerminal",
      "type": "node",
      "presentation": {
        "hidden": false,
        "group": "launch Node",
        "order": 1
      }
    },
    {
      "name": "Ë∞ÉËØï eslint api",
      "program": "${workspaceFolder}/packages/shell-debug-demo/eslintApiDebugger.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal",
      "type": "node",
      "presentation": {
        "hidden": false,
        "group": "launch Node",
        "order": 1
      }
    },
    {
      "name": "Ë∞ÉËØï babel api",
      "program": "${workspaceFolder}/packages/babel-debug-demo/index.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/packages/babel-debug-demo",

      "type": "node",
      "resolveSourceMapLocations": ["${workspaceFolder}/**"],
      "presentation": {
        "hidden": false,
        "group": "launch Node",
        "order": 1
      }
    },
    {
      "name": "Launch Node Program",
      "program": "${workspaceFolder}/packages/node-debug-demo/index.js",
      "request": "launch",
      "args": ["--aaa", "--aaa2"],
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"],
      "stopOnEntry": true, //È¶ñË°åÊñ≠
      "presentation": {
        "hidden": false,
        "group": "launch Node",
        "order": 1
      },
      "type": "node"
    },
    {
      "name": "Attach Node",
      "port": 8888,
      "request": "attach",
      "restart": {
        "delay": 1000,
        "maxAttempts": 3
      },
      "autoAttachChildProcesses": true,
      "skipFiles": ["<node_internals>/**"],
      "presentation": {
        "hidden": false,
        "group": "attach Node",
        "order": 1
      },
      "type": "node"
    },

    {
      "name": "Launch React webpack",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:5555",
      "webRoot": "${workspaceFolder}",
      "file": "${workspaceFolder}/packages/webpack-react-debug/index.html",
      "presentation": {
        "hidden": false,
        "group": "launch chrome",
        "order": 1
      },
      "sourceMapPathOverrides": {
        "meteor://üíªapp/*": "${workspaceFolder}/packages/*",
        "webpack:///./~/*": "${workspaceFolder}/node_modules/*",
        "webpack://*": "${workspaceFolder}/packages/*"
      }
    },
    {
      "name": "Launch vue3",
      "request": "launch",
      "type": "chrome",
      // "runtimeArgs": ["--auto-open-devtools-for-tabs "],
      "userDataDir": false,
      "runtimeExecutable": "canary",
      "presentation": {
        "hidden": false,
        "group": "launch chrome",
        "order": 1
      },
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Launch vue webpack",
      "request": "launch",
      "type": "chrome",
      "runtimeExecutable": "canary",
      "presentation": {
        "hidden": false,
        "group": "launch chrome",
        "order": 1
      },
      "userDataDir": false,
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "meteor://üíªapp/*": "${workspaceFolder}/packages/*",
        "webpack:///./~/*": "${workspaceFolder}/node_modules/*",
        "webpack://*": "${workspaceFolder}/packages/*"
      }
    },
    {
      "name": "Attach to Chrome",
      "port": 9222,
      "request": "attach",
      "type": "chrome",
      "presentation": {
        "hidden": false,
        "group": "Attach chrome",
        "order": 1
      },
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Launch react",
      "request": "launch",
      "runtimeExecutable": "canary",
      "presentation": {
        "hidden": false,
        "group": "launch chrome",
        "order": 1
      },
      // "runtimeArgs": ["--auto-open-devtools-for-tabs "],
      "userDataDir": false,
      "type": "chrome",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
